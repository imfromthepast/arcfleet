minPlotLength = 2;\n\t\t            var maxPlotLength = 25;\n\t\t            var plotArray = [];\n\t\t            $scope.result = '';\n\t\t            $scope.seed = 0;\n\t\t            $scope.generatePlot = function(ran){\n\t\t            \tplotArray = [];\n\t\t            \tplotArray.push(0);\n\t\t            \tvar currentPlotPoint = 0;\n\t\t            \t$scope.result = plotPointArray[currentPlotPoint].description;\n\t\t            \tfor (var i = 0; i < maxPlotLength; i++) {\n\t\t            \t\tcurrentPlotPoint = $scope.generateNextPlotPoint(currentPlotPoint,ran[i]);\n\t\t            \t\tif(currentPlotPoint == -1){\n\t\t            \t\t\tif(i>minPlotLength){\n\t\t            \t\t\t\treturn $scope.result;\n\t\t            \t\t\t}else{\n\t\t            \t\t\t\tcurrentPlotPoint = $scope.randomPlotPoint(ran[i]);\n\t\t            \t\t\t}\n\t\t            \t\t}\n\t\t            \t\t$scope.result += \" \"+plotPointArray[currentPlotPoint].description+\" (\"+currentPlotPoint+\")\";\n\t\t            \t\tplotArray.push(currentPlotPoint);\n\t\t            \t}\n\t\t            \tconsole.log(plotArray);\n\t\t            }\n\t\t            $scope.generateRandomPlot = function(){\n\t\t            \tvar ran = [];\n\t\t            \tvar square = Date.now();\n\t\t            \tvar squareString = square.toString().replace('0','');\n\t\t            \tvar pp = squareString;\n\t\t            \tfor (var i = 0; i < maxPlotLength; i++) {\n\t\t            \t\tsquare = (pp * 2);\n\t\t            \t\tsquareString = square.toString().replace('0','');\n\t\t            \t\tpp = squareString; \n\t\t            \t\tran.push(randomIntFromInterval(0,25));\n\t\t            \t}\n\t\t            \t$scope.generatePlot(ran);\n\t\t            }\n\t\t            $scope.plotPointActive = function(p){\n\t\t            \treturn plotArray.indexOf(p) > -1?'badge':'';\n\t\t            }\n\t\t        }]);\n\t\t\t\tfunction randomIntFromInterval(min,max){\n\t\t\t\t    return Math.floor(Math.random()*(max-min+1)+min);\n\t\t\t\t}\n\t\t    </script> \n\t\t    <style>\n\t\t\t\t.box{\n\t\t\t\t\tpadding: 2px;\n\t\t\t\t\tborder: 1px solid #E4E0E0;\n\t\t\t\t}\t\t\t\t\n\t            @font-face {\n\t                font-family: 'ARCFleet';\n\t                src: url(\"{!URLFOR($Resource.ARCFleetResources,'/fonts/SFAlienEncountersSolid.ttf')}\");\n\t            }\n\t            @font-face {\n\t                font-family: 'ARCFleetLined';\n\t                src: url(\"{!URLFOR($Resource.ARCFleetResources,'/fonts/SFAlienEncounters.ttf')}\");\n\t            }\n\t\t    </style>\n    \t</head>\n    \t<body>\n\t    \t<div class=\"container\">\n\t            <div class=\"panel panel-default\">\n\t                <div class=\"panel-heading\">\n\t                    <h2 class=\"panel-title\">\n\t                        Markov Chains\n\t                    </h2>\n\t                </div>\n\t                <div class=\"panel-body\">\n\t\t\t\t\t    <div id=\"ng-app\" ng-app=\"app\" ng-controller=\"cont\" ng-cloak=\"true\">\n\t\t\t\t\t\t\t<input class=\"form-control\" ng-model=\"seed\" type=\"number\"/>\n\t\t\t\t\t\t\t<button class=\"btn btn-default\" ng-click=\"generateRandomPlot()\">Generate Random</button>\n\t\t\t\t\t\t\t<button class=\"btn btn-default\" ng-click=\"generatePlot(seed)\">Generate</button>\n\t\t\t\t\t\t\t\n\t\t\t\t\t    \t<div class=\"row\">\n\t\t\t\t\t    \t\t<div class=\"col-xs-5\" ng-bind-html=\"result\"></div>\n\t\t\t\t\t    \t\t<div class=\"col-xs-7\">\n\t\t\t\t\t\t    \t\t<div class=\"row text-center\">\n\t\t\t\t\t\t\t    \t\t<div class=\"col-xs-12\">\n\t\t\t\t\t\t\t\t\t\t    <div class=\"row text-center\">\n\t\t\t\t\t\t\t\t\t\t    \t<div class=\"col-xs-9 box\"><div ng-class=\"plotPointActive(0)\">0</div></div>\n\t\t\t\t\t\t\t\t\t\t    </div>\n\t\t\t\t\t\t\t\t\t\t\t<div class=\"row text-center\">\n\t\t\t\t\t\t\t\t\t\t\t\t<div class=\"col-xs-3 box\"><div ng-class=\"plotPointActive(1)\">1</div></div>\n\t\t\t\t\t\t\t\t\t\t\t\t<div class=\"col-xs-3 box\"><div ng-class=\"plotP