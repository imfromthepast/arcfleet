		            $scope.generateNextPlotPoint = function(plotPoint,ran){
		            	var transitionArray = $scope.transitionTable[plotPoint];
		            	var p = ran % transitionArray.length;
		            	return transitionArray[p];
		            }
		            $scope.randomPlotPoint = function(ran){
		            	var keys = Object.keys($scope.transitionTable);
		            	var ranA = keys[ran % keys.length];
		            	return ranA;
		            }
		            var minPlotLength = 2;
		            var maxPlotLength = 25;
		            var plotArray = [];
		            $scope.result = '';
		            $scope.seed = 0;
		            $scope.generatePlot = function(ran){
		            	plotArray = [];
		            	plotArray.push(0);
		            	var currentPlotPoint = 0;
		            	$scope.result = plotPointArray[currentPlotPoint].description;
		            	for (var i = 0; i < maxPlotLength; i++) {
		            		currentPlotPoint = $scope.generateNextPlotPoint(currentPlotPoint,ran[i]);
		            		if(currentPlotPoint == -1){
		            			if(i>minPlotLength){
		            				return $scope.result;
		            			}else{
		            				currentPlotPoint = $scope.randomPlotPoint(ran[i]);
		            			}
		            		}
		            		$scope.result += " "+plotPointArray[currentPlotPoint].description+" ("+currentPlotPoint+")";
		            		plotArray.push(currentPlotPoint);
		            	}
		            	console.log(plotArray);
		            }
		            $scope.generateRandomPlot = function(){
		            	var ran = [];
		            	var square = Date.now();
		            	var squareString = square.toString().replace('0','');
		            	var pp = squareString;
		            	for (var i = 0; i < maxPlotLength; i++) {
		            		square = (pp * 2);
		            		squareString = square.toString().replace('0','');
		            		pp = squareString; 
		            		ran.push(randomIntFromInterval(0,25));
		            	}
		            	$scope.generatePlot(ran);
		            }
		            $scope.plotPointActive = function(p){
		            	return plotArray.indexOf(p) > -1?'badge':'';
		            }
		        }]);
				function randomIntFromInterval(min,max){
				    return Math.floor(Math.random()*(max-min+1)+min);
				}
