            <script>
        // ANGULAR.JS
            // Name your application
                var app = angular.module('app', []);
            // Define Controllers
                var con = app.controller('con', function ($scope, $filter) {
            // Initialize Scope Variables
                    $scope.t;
                    $scope.to = 1;
                    $scope.v = 0.9;
                    $scope.c = 1;
                    $scope.solve = function(){
                        if($scope.v < 1 && $scope.v > 0){
                            $scope.t = $scope.to / Math.pow(( (1-($scope.v*$scope.v)) ), 0.5);
                            $scope.message = 'Traveling at '+$scope.v+' the speed of light, '+ $scope.t.toFixed(3) +' years will pass on earth for every year on your spaceship.';
                        }else if($scope.v < 0){
                            $scope.message = $scope.v + ' the speed of light? What does that even mean? And don\'t tell me it means you\'re going backwards. That\'s dumb and you should feel bad for even thinking it.';
                        }else if($scope.v == 0){
                            $scope.message = '0? Really? Quit wasting my time.';
                        }else if($scope.v == 42){
                            $scope.message = 'That won\'t work without your towel.';
                        }else if($scope.v > 1){
                            $scope.message = 'Dude, this isn\'t Star Trek, take that mess elsewhere.';
                        }else if($scope.v == 1){
                            $scope.message = '"To Infinity! And Bey-" ugh, nope, not going to do it.';
                        }
                    }
                    $scope.solve();
                });
            </script>  
