        <body>
            <div class="container">
                <div class="panel panel-default">
                    <div class="panel-heading">
                        <h1 class="panel-title">
                            Time Dilation
                        </h1>
                    </div>
                    <div class="panel-body">
                        <form class="form-horizontal">
                            <div class="help-block">Enter a decimal from 0 - 1 in the field below to calculate your time dialation.</div>
                            <div class="form-group col-sm-12">
                                <label for="travelTime" class="col-sm-4 control-label">Times the Speed of Light</label>
                                <div class="col-sm-8">
                                    <input type="text" class="form-control" ng-max="1" id="travelTime" ng-model="v" ng-keyup="solve()" placeholder="0.5"/>
                                </div>
                            </div>
                            <div class="col-xs-12">
                                <h3>{{message}}</h3>
                            </div>
                        </form>
                    </div>
                </div>
            </div>
        </body>
    </html>
