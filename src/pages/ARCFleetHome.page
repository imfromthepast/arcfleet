            <style> 
                .navbar-flat{background: #3c3c3c !important;}                
                .construct{
                    position: absolute; 
                    top: 15px; 
                    left: 30px; 
                    pointer-events: none;
                    z-index: 10;
                    font-size: 250px;
                    opacity: 0.45;
                }
                .construct::after{
                    position: absolute;
                    left: 0;
                    top: 0;
                    z-index: -1;
                    -webkit-text-stroke: 4px #000;
                }

                .construct-regular::after{content: "\e605";}
                .construct-explorer::after{content: "\e601";}
                .construct-destroyer::after{content: "\e603";}
                .construct-arcbase::after{content: "\e606";}
                .arc-explorer{color:#5A8EF9;}
                .arc-regular{color: #797979;}
                .arc-destroyer{color:#D62C2C;}
                .arc-arcbase{color: #A6A6A6;}
                /*.civ{cursor: pointer;}
                .civ:hover{color: #32a2c7;}*/
                /* -- custom css for Bootstrap 3.x --*/
                /* move special fonts to HTML head for better performance */
                @import url(http://fonts.googleapis.com/css?family=Voltaire);
                html,body{height: 100%;width: 100%;font-family:Arial,sans-serif;}
                body{
                    background-color: #000;
                    background-image: url('{!URLFOR($Resource.starBG)}');
                    background-repeat:no-repeat;
                    background-attachment:fixed;
                }
                /* fix bs3 horizontal scrollbar bug */
                .row{margin: 0;padding: 0;}
                /* use special fonts in certain elements */                
                h1,h2,h3,h4,.lead,.btn,.navbar a{
                  text-transform:uppercase;
                  font-family:'Voltaire',Arial,sans-serif;
                  letter-spacing:2px;
                }
                h1{font-size:50px;}
                
                .navbar a {
                  color:#fff;
                  font-size:14px;
                }
                .navbar-bold {
                  background-color:#f68076;
                  font-family:'Helvetica Neue',Helvetica,Arial,sans-serif;
                }
                .navbar-bold li a:hover, .navbar-bold li.active {
                  background-color:#d46054;
                }                  

                .header .btn-lg {
                   font-size:28px;
                   border-color:#eeeeee;
                   padding:15px;
                   background-color:transparent;
                   color:#ffffff;
                }
                .header .btn-lg:hover {
                   background-color:#eeeeee;
                   color:#777777;
                } 
                .header h1 {font-size:80px;-webkit-text-stroke: 1px rgba(f,f,f,0.1);color:#431a6d;color:#fff;margin-left:-5px;margin-bottom:5px;text-transform:uppercase;} 
                .vert {vertical-align: middle;width:100%;text-align:center;}
                .header .lead{color:#60c7c6;font-size:25px;}

                .feature h1,.feature h2,.feature h3,.feature h4,.feature h5{color: #393939;}                
                .feature {background-color:#dbdbdb;}
                .feature .panel {background-color:transparent;}
                .feature .blurb{font-size: 16px;margin-top: 20px;border-top: 1px solid #99999A;}
                .feature .blurb p{padding: 40px 0;}
                
                .release{background: #dbdbdb;color:#777;}

                .gameplay {color: #ffffff;background-image: url('{!URLFOR($Resource.ARCFleetResources,"/img/galaxy-wallpapers-10.jpg")}');background-color: #000;background-repeat:no-repeat;background-attachment:fixed;}
                .gameplay-item{margin-bottom: 15px;}
                .gameplay-item{padding: 10px;border-radius: 5px;background: rgba(0,0,0,0.5);height: 280px;}
                .gameplay-item p{font-size: 24px;font-family: "HelveticaNeue-Light", "Helvetica Neue Light", "Helvetica Neue", Helvetica, Arial, "Lucida Grande", sans-serif;font-weight: 300;}
                .gameplay-item .blurb{color: #ffffff;z-index: 20;}
                .section{padding: 80px 0;}

                footer {padding: 100px 0;background: #434343;color: #c8c8c8;}
                .panelButton{background: #3c3c3c;border-color: #000000 !important;box-shadow: inset 1px 1px 1px rgba(255,255,255,.15),0 1px 5px rgba(0,0,0,.075) !important;color: #999 !important;}
                #grid[data-columns='data-columns']::before {
                    content: '3 .column.size-1of3';
                }
                div.img-div{                    
                    overflow:hidden;
                    border-radius:50%;
                }
                .carousel-fade .carousel-inner .item {
                  opacity: 0;
                  -webkit-transition-property: opacity;
                          transition-property: opacity;
                }
                .carousel-fade .carousel-inner .active {
                  opacity: 1;
                }
                .carousel-fade .carousel-inner .active.left,
                .carousel-fade .carousel-inner .active.right {
                  left: 0;
                  opacity: 0;
                  z-index: 1;
                }
                .carousel-fade .carousel-inner .next.left,
